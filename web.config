<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="PythonHandler" 
           path="*" 
           verb="*" 
           modules="httpPlatformHandler" 
           resourceType="Unspecified" />
    </handlers>
    <httpPlatform processPath="D:\home\python310\python.exe"
                  arguments="app.py"
                  stdoutLogEnabled="true"
                  stdoutLogFile="D:\home\LogFiles\python.log"
                  startupTimeLimit="300"
                  processesPerApplication="1">
      <environmentVariables>
        <environmentVariable name="PYTHONPATH" value="D:\home\site\wwwroot" />
        <environmentVariable name="AZURE_STORAGE_CONNECTION_STRING" 
                             value="DefaultEndpointsProtocol=https;AccountName=carplatmodels123;AccountKey=YOUR_KEY;EndpointSuffix=core.windows.net" />
      </environmentVariables>
    </httpPlatform>
  </system.webServer>
</configuration>
